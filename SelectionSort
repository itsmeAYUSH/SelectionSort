        for(int i=0 ; i < arr.length-1 ; i++){
            int max = arr[i];
            int index = i;
            for(int j=i+1 ; j< arr.length ; j++){
                if(arr[j] > max){
                    max = arr[j];
                    index =j;
                }
            }int temp = arr[i];
            arr[i]=arr[index];
            arr[index] = temp;

        }return (arr);
